                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:backgroundTint="@color/stroke_color"
                        android:inputType="textEmailAddress"
                        android:singleLine="true"
                        android:textColor="@color/white"
                        android:textSize="15sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/password"
                        android:textColor="@color/blue_color"
                        android:textSize="16sp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <EditText
                            android:id="@+id/eT_login_pswd"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:backgroundTint="@color/stroke_color"
                            android:inputType="textPassword"
                            android:maxLines="1"
                            android:paddingRight="20sp"
                            android:singleLine="true"
                            android:textColor="@color/white"
                            android:textSize="15sp" />

                        <ImageView
                            android:layout_width="22sp"
                            android:layout_height="22sp"
                            android:layout_alignRight="@+id/eT_login_pswd"
                            android:layout_centerVertical="true"
                            android:background="@drawable/ic_show_password" />


                    </RelativeLayout>

                </LinearLayout>

                <Button
                    android:id="@+id/btn_signIn"
                    android:layout_width="@dimen/space_120"
                    android:layout_height="@dimen/space_40"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/space_30"
                    android:background="@drawable/shape_rounded_blue"
                    android:gravity="center"
                    android:paddingLeft="@dimen/space_30"
                    android:paddingRight="@dimen/space_30"
                    android:text="@string/sign_in"
                    android:textColor="@color/white" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/space_20"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/space_50">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/space_5"
                        android:text="If you forget your Password then reset Password at your manufacturer Website/app"
                        android:textAlignment="center"
                        android:textColor="@color/text_color_dim"
                        android:textSize="@dimen/space_16" />

                </LinearLayout>


            </LinearLayout>


        </LinearLayout>

        <RelativeLayout
            android:id="@+id/band_fifth"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_horizontal"
                android:orientation="vertical">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30sp"
                    android:text="Registration successfully"
                    android:textColor="@color/white"
                    android:textSize="22sp" />

                <ImageView
                    android:layout_width="80sp"
                    android:layout_height="80sp"
                    android:layout_marginTop="25sp"
                    android:src="@drawable/ic_wearable" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30sp"
                    android:text="Band 1"
                    android:textColor="@color/white"
                    android:textSize="20sp" />

                <ImageView
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginTop="25sp"
                    android:src="@drawable/ic_done_blue" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15sp"
                    android:layout_marginRight="15sp"
                    android:layout_marginTop="25sp"
                    android:text="Proceed to Subscription to start using this Band"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="16sp" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="horizontal">

                    <android.support.v7.widget.CardView
                        android:id="@+id/later"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5sp"
                        android:layout_marginTop="40sp"
                        app:cardBackgroundColor="@color/blue_color"
                        app:cardCornerRadius="22sp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="8sp"
                            android:layout_marginLeft="20sp"
                            android:layout_marginRight="20sp"
                            android:layout_marginTop="8sp"
                            android:text="SUBSCRIB LATER"
                            android:textColor="@color/white"
                            android:textSize="16sp" />

                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                        android:id="@+id/now"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5sp"
                        android:layout_marginTop="40sp"
                        app:cardBackgroundColor="@color/blue_color"
                        app:cardCornerRadius="22sp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="8sp"
                            android:layout_marginLeft="20sp"
                            android:layout_marginRight="20sp"
                            android:layout_marginTop="8sp"
                            android:text="SUBSCRIB NOW"
                            android:textColor="@color/white"
                            android:textSize="16sp" />

                    </android.support.v7.widget.CardView>
                </LinearLayout>


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15sp"
                    android:layout_marginRight="15sp"
                    android:layout_marginTop="15sp"
                    android:text="Already Subscribed"
                    android:textAlignment="center"
                    android:textColor="@color/stroke_color"
                    android:textSize="15sp" />


            </LinearLayout>


        </RelativeLayout>

    </RelativeLayout>


</FrameLayout>